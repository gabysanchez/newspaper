<!DOCTYPE html>
<html lang="es">

<head>
    <%- include('./partials/head'); %>
</head>

<body>
    <header>
        <%- include('./partials/nav'); %>
    </header>
    <main>
        <h2>NEWS</h2>
        <section>
            <ul>
                <% data.map((news)=>{ %>
                    <li>
                        <p>ID: <span>
                                <%=news._id %>
                            </span></p>
                        <p>TITLE: <span>
                                <%=news.title %>
                            </span>
                        </p>
                    </li>
                    <% }) %>
            </ul>
        </section>

    </main>
</body>


</html>